"filePath":"109","messages":"110","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"83"},"/home/kelvin/Development/code/phase-2/react-recipe-project/src/index.js",[],"/home/kelvin/Development/code/phase-2/react-recipe-project/src/App.js",[],"/home/kelvin/Development/code/phase-2/react-recipe-project/src/reportWebVitals.js",[],"/home/kelvin/Development/code/phase-2/react-recipe-project/src/components/About/About.js",[],"/home/kelvin/Development/code/phase-2/react-recipe-project/src/components/Connect/Connect.js",[],"/home/kelvin/Development/code/phase-2/react-recipe-project/src/components/NotFound/NotFound.js",[],"/home/kelvin/Development/code/phase-2/react-recipe-project/src/components/Home/Home.js",["111"],"/home/kelvin/Development/code/phase-2/react-recipe-project/src/components/OrderReviews/Review/Review.js",["112","113"],"/home/kelvin/Development/code/phase-2/react-recipe-project/src/components/meals/DisplayMeal/DisplayMeal.js",[],"/home/kelvin/Development/code/phase-2/react-recipe-project/src/components/meals/AllMeal/AllMeal.js",[],"/home/kelvin/Development/code/phase-2/react-recipe-project/src/components/OrderReviews/ReviewCard/ReviewCard.js",[],"/home/kelvin/Development/code/phase-2/react-recipe-project/src/components/Shares/NavBar/NavBar.js",["114"],"/home/kelvin/Development/code/phase-2/react-recipe-project/src/components/ModalPop/ModalPop.js",["115"],"/home/kelvin/Development/code/phase-2/Mapishi-mema/src/index.js",[],["116","117"],"/home/kelvin/Development/code/phase-2/Mapishi-mema/src/App.js",[],"/home/kelvin/Development/code/phase-2/Mapishi-mema/src/reportWebVitals.js",[],"/home/kelvin/Development/code/phase-2/Mapishi-mema/src/components/Home/Home.js",["118"],"import React, { useContext, useEffect, useState } from \"react\";\nimport { Container, Button } from \"react-bootstrap\";\nimport { FoodContext } from \"../../App\";\nimport AllMeal from \"../meals/AllMeal/AllMeal\";\nimport NavBar from \"../Shares/NavBar/NavBar\";\nimport SyncLoader from \"react-spinners/SyncLoader\";\nimport toast, { Toaster } from \"react-hot-toast\";\n\nconst Home = () => {\n  const [allMeal, setAllMeal] = useState([]);\n  const [search, setSearch] = useState(\"\");\n  const [card, setCard] = useState([]);\n  const [foodInfo, setFoodInfo] = useContext(FoodContext);\n\n  // Enter-key-active\n  const enterKeyPress = (event) => {\n    if (event.key === \"Enter\") {\n      document.getElementById(\"search-btn\").click();\n    }\n  };\n\n  //search-filed and click\n  const handelClick = () => {\n    const searchText = document.getElementById(\"search-text\").value;\n    document.getElementById(\"search-text\").value = \"\";\n    console.log(searchText);\n    setSearch(searchText);\n  };\n\n  useEffect(() => {\n    fetch(`https://www.themealdb.com/api/json/v1/1/search.php?s=${search}`)\n      .then((res) => res.json())\n      .then((data) => {\n        if (data.meals) {\n          setAllMeal(data.meals);\n        } else {\n          toast.error(\"Your search is not Found\", {\n            duration: 4000,\n          });\n        }\n      });\n  }, [search]);\n\n  // addToFavourites\n  const handleClick = (meal) => {\n    const newCard = [...card, meal];\n    setCard(newCard);\n    setFoodInfo(newCard);\n  };\n\n  return (\n    <>\n      <NavBar />\n      <Toaster />\n      <Container className=\"text-center mt-3\">\n        <div className=\"row p-5\">\n          <div className=\"col-md-6 mx-auto d-flex justify-content-center align-items-center\">\n            <input\n              id=\"search-text\"\n              className=\"form-control rounded-3\"\n              onKeyPress={enterKeyPress}\n              type=\"text\"\n              name=\"text\"\n              placeholder=\"search your meal\"\n            />\n\n            <Button\n              id=\"search-btn\"\n              onClick={handelClick}\n              className=\"btn btn-main \"\n            >\n              Search\n            </Button>\n          </div>\n        </div>\n\n        {allMeal.length === 0 ? (\n          <div className=\"d-flex align-content-center justify-content-center p-5\">\n            <SyncLoader\n              color={\"#36D7B7\"}\n              loading={allMeal}\n              width={8}\n              height={18}\n              radius={10}\n              margin={10}\n            />\n          </div>\n        ) : (\n          <div className=\"mt-2 d-flex align-content-center justify-content-center \">\n            <div className=\"row \">\n              {allMeal.map((meal) => (\n                <AllMeal\n                  handleClick={handleClick}\n                  meal={meal}\n                  key={meal.idMeal}\n                />\n              ))}\n            </div>\n          </div>\n        )}\n      </Container>\n    </>\n  );\n};\n\nexport default Home;\n","/home/kelvin/Development/code/phase-2/Mapishi-mema/src/components/Connect/Connect.js",[],"/home/kelvin/Development/code/phase-2/Mapishi-mema/src/components/NotFound/NotFound.js",[],"/home/kelvin/Development/code/phase-2/Mapishi-mema/src/components/About/About.js",[],"/home/kelvin/Development/code/phase-2/Mapishi-mema/src/components/meals/DisplayMeal/DisplayMeal.js",[],"/home/kelvin/Development/code/phase-2/Mapishi-mema/src/components/OrderReviews/Review/Review.js",["119","120"],"/home/kelvin/Development/code/phase-2/Mapishi-mema/src/components/OrderReviews/ReviewCard/ReviewCard.js",[],"/home/kelvin/Development/code/phase-2/Mapishi-mema/src/components/Shares/NavBar/NavBar.js",["121"],"import React, { useContext, useState } from \"react\";\nimport { Container, Navbar, Nav } from \"react-bootstrap\";\nimport { Link } from \"react-router-dom\";\nimport { FontAwesomeIcon } from \"@fortawesome/react-fontawesome\";\nimport { faHeart } from \"@fortawesome/free-solid-svg-icons\";\nimport \"./NavBar.css\";\nimport { FoodContext } from \"../../../App\";\nimport ModalPop from \"../../ModalPop/ModalPop\";\n\nconst NavBar = () => {\n  const [foodInfo, setFoodInfo] = useContext(FoodContext);\n  const [smShow, setSmShow] = useState(false);\n\n  return (\n    <>\n      <Navbar bg=\"light\" expand=\"lg\">\n        <Container>\n          <Navbar.Brand href=\"#home\">\n            <h1>Moringa Foods</h1>\n          </Navbar.Brand>\n          <Navbar.Toggle aria-controls=\"basic-navbar-nav\" />\n          <Navbar.Collapse id=\"basic-navbar-nav\">\n            <Nav className=\"ms-auto\">\n              <Nav.Link className=\"ms-4\" as={Link} to=\"/home\">\n                Home\n              </Nav.Link>\n              <Nav.Link className=\"ms-4\" as={Link} to=\"/review\">\n                Favourites\n              </Nav.Link>\n              <Nav.Link className=\"ms-4\" as={Link} to=\"/about\">\n                About Us\n              </Nav.Link>\n              <Nav.Link className=\"ms-4\" as={Link} to=\"/contact\">\n                Contact\n              </Nav.Link>\n\n              <Nav.Link className=\"border-0 bg-transparent position-relative ms-4\">\n                <FontAwesomeIcon className=\"text-main\" icon={faHeart} />\n                <span\n                  onClick={() => setSmShow(!smShow)}\n                  className=\"position-absolute top-0 start-100 translate-middle badge rounded-pill bg-success\"\n                >\n                  {foodInfo.length}\n                </span>\n              </Nav.Link>\n            </Nav>\n          </Navbar.Collapse>\n        </Container>\n      </Navbar>\n\n      <ModalPop smShow={smShow} setSmShow={setSmShow} />\n    </>\n  );\n};\n\nexport default NavBar;\n","/home/kelvin/Development/code/phase-2/Mapishi-mema/src/components/ModalPop/ModalPop.js",["122"],"//To remove from favourite\nimport React, { useContext, useState } from \"react\";\nimport { Modal } from \"react-bootstrap\";\nimport { FoodContext } from \"../../App\";\nimport { FontAwesomeIcon } from \"@fortawesome/react-fontawesome\";\nimport { faXmark } from \"@fortawesome/free-solid-svg-icons\";\nimport swal from \"sweetalert\";\n\nconst ModalPop = ({ smShow, setSmShow }) => {\n  const [foodInfo, setFoodInfo] = useContext(FoodContext);\n\n  const handelRemove = (id) => {\n    const remainingMeal = foodInfo.filter((food) => food.idMeal !== id);\n    setFoodInfo(remainingMeal);\n    swal(\"Successfully Removed!\", \"Your Meal has been successfully Removed.\", {\n      icon: \"success\",\n    });\n  };\n\n  return (\n    <>\n      <Modal size=\"lg\" show={smShow} onHide={() => setSmShow(false)}>\n        <Modal.Header closeButton>\n          <Modal.Title className=\"text-muted\">Your Cart </Modal.Title>\n        </Modal.Header>\n\n        <Modal.Body className=\"mt-3\">\n          {foodInfo.map((food) => (\n            <div className=\"d-flex justify-content-between align-items-center\">\n              <ul className=\"list-unstyled text-main fs-5\">\n                <li>{food.strMeal}</li>\n              </ul>\n\n              <FontAwesomeIcon\n                onClick={() => handelRemove(food.idMeal)}\n                className=\"text-danger cursor fs-5\"\n                icon={faXmark}\n              />\n            </div>\n          ))}\n        </Modal.Body>\n      </Modal>\n    </>\n  );\n};\n\nexport default ModalPop;\n","/home/kelvin/Development/code/phase-2/Mapishi-mema/src/components/meals/AllMeal/AllMeal.js",[],{"ruleId":"123","severity":1,"message":"124","line":13,"column":10,"nodeType":"125","messageId":"126","endLine":13,"endColumn":18},{"ruleId":"123","severity":1,"message":"127","line":1,"column":29,"nodeType":"125","messageId":"126","endLine":1,"endColumn":38},{"ruleId":"123","severity":1,"message":"128","line":1,"column":40,"nodeType":"125","messageId":"126","endLine":1,"endColumn":48},{"ruleId":"123","severity":1,"message":"129","line":11,"column":20,"nodeType":"125","messageId":"126","endLine":11,"endColumn":31},{"ruleId":"123","severity":1,"message":"128","line":2,"column":29,"nodeType":"125","messageId"